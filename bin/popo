#!/usr/bin/env php
<?php

require_once getcwd().'/vendor/autoload.php';

use Popo\Command\GenerateDtoCommand;
use Popo\Command\GeneratePopoCommand;
use Symfony\Component\Console\Application;

$console = new Application('popo', 'v2.x');
$console->add(new GeneratePopoCommand());
$console->add(new GenerateDtoCommand());

$console->run();
