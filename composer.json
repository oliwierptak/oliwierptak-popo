{
  "name": "popo/generator",
  "description": "Plain Old PHP Object (POPO) / Value Object (VO) / Data Transfer Object (DTO) Generator. Convert (JSON) schema to PHP class.",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Oliwier Ptak"
    }
  ],
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": "^7.4",
    "ext-json": "*",
    "symfony/finder": "^5",
    "symfony/console": "^5"
  },
  "require-dev": {
    "phpunit/phpunit": "^8",
    "everon/coding-standard": "^1",
    "symfony/var-dumper": "^5"
  },
  "autoload": {
    "psr-4": {
      "Popo\\": "src/Popo"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "TestsSuites\\Popo\\": "tests/suits",
      "TestsStubs\\Popo\\": "tests/stubs",
      "App\\": "tests/App"
    }
  },
  "scripts": {
    "popo": "bin/popo generate --showConfiguration 1 --showConfigurationBorder 1 --showProgressBar 0 -c tests/fixtures/.popo",
    "post-autoload-dump": [
      "chmod +x bin/popo",
      "vendor/bin/cs-setup"
    ]
  },
  "scripts-descriptions": {
    "popo": "Generate POPO files"
  },
  "bin": [
    "bin/popo"
  ]
}
